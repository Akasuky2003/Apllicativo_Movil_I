<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectoobregon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTemamainTercero">
        <activity
            android:name=".ContactoActivity"
            android:exported="false" />
        <activity
            android:name=".DetalleTrabajadorActivity"
            android:exported="false" />
        <activity
            android:name=".TrabajadorActivity"
            android:exported="false"
            android:label="Modulo de Trabajadores"
            android:theme="@style/AppTemamainSecundario" />
        <activity
            android:name=".AyudaActivity"
            android:exported="false" />
        <activity
            android:name=".UsuarioActivity"
            android:exported="false"
            android:label="Modulo de Usuarios"
            android:theme="@style/AppTemamainSecundario" />
        <activity
            android:name=".DetalleClienteActivity"
            android:exported="false"
            android:label="Detalle de Cliente"
            android:theme="@style/AppTemamainSecundario" />
        <activity
            android:name=".ProductoActivity"
            android:exported="false"
            android:label="Modulo de Productos"
            android:theme="@style/AppTemamainSecundario" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClientesActivity"
            android:exported="false"
            android:label="Modulo de Clientes"
            android:theme="@style/AppTemamainSecundario" />
        <activity
            android:name=".MenuPrincipalActivity"
            android:exported="false"
            android:theme="@style/AppTemamainTercero" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/AppTemamainTercero" />
    </application>

</manifest>